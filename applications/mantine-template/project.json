{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "applications/mantine-template",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "applications/mantine-template",
        "outputPath": "dist/applications/mantine-template"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "mantine-template:build",
        "dev": true,
        "port": 4201
      },
      "configurations": {
        "development": {
          "buildTarget": "mantine-template:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "mantine-template:build:production",
          "dev": false
        }
      }
    },
    "analyze": {
      "executor": "nx:run-commands",
      "outputs": ["{options.outputPath}"],
      "options": {
        "commands": ["export ANALYZE=\"true\" && nx run mantine-template:build"]
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "mantine-template:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/applications/mantine-template"],
      "options": {
        "jestConfig": "applications/mantine-template/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "applications/mantine-template/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  },
  "tags": []
}
